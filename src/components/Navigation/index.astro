---
import './style.scss';

const baseUrl = import.meta.env.BASE_URL;
const navigation = [
  { name: 'Roadmap', href: '' },
];
---

<div class='nav-links'>
  {
    navigation.map((item) => (
      <a
        class='text-sm font-semibold leading-6 text-gray-900'
        href={`${baseUrl + item.href}`}
      >
        {item.name}
      </a>
    ))
  }
</div>

<script src='./menu.ts'></script>
